{
  "version": "0.0.0",
  "name": "@gazatu/solid-spectre",
  "description": "Solid.js components styled with Spectre.css",
  "license": "MIT",
  "author": "GaZaTu",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GaZaTu/solid-spectre.git"
  },
  "homepage": "https://github.com/GaZaTu/solid-spectre#readme",
  "bugs": {
    "url": "https://github.com/GaZaTu/solid-spectre/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite serve dev",
    "tsc": "tsc",
    "build": "node build.js",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.7.0"
  },
  "dependencies": {
    "@felte/solid": "^1.2.10",
    "@solid-primitives/intersection-observer": "^2.1.2",
    "@solid-primitives/media": "^2.2.3",
    "@solid-primitives/memo": "^1.3.1",
    "@solid-primitives/props": "^3.1.6",
    "@solid-primitives/storage": "^2.0.0",
    "@tanstack/solid-table": "^8.9.3",
    "@thisbeyond/solid-select": "^0.13.0",
    "feather-icons": "^4.29.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-plugin-solid": "^0.7.3",
    "glob": "^10.3.2",
    "solid-js": "^1.7.7",
    "typescript": "^5.1.6",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "zx": "^7.2.3"
  },
  "browser": {},
  "exports": {
    "./ui/*": {
      "types": "./dist/ui/*.d.ts",
      "import": "./dist/ui/*.jsx"
    },
    "./util/*": {
      "types": "./dist/util/*.d.ts",
      "import": "./dist/util/*.jsx"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "import": "./dist/icons/*.jsx"
    }
  },
  "typesVersions": {
    "*": {
      "ui/*": [
        "dist/ui/*"
      ],
      "util/*": [
        "dist/util/*"
      ],
      "icons/*": [
        "dist/icons/*"
      ]
    }
  }
}
