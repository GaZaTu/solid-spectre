{
  "version": "0.0.0",
  "name": "solid-spectre",
  "description": "Solid.js components styled with Spectre.css",
  "license": "MIT",
  "author": "GaZaTu",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GaZaTu/solid-spectre.git"
  },
  "homepage": "https://github.com/GaZaTu/solid-spectre#readme",
  "bugs": {
    "url": "https://github.com/GaZaTu/solid-spectre/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite serve dev",
    "tsc": "tsc",
    "build": "node build.js",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.7.0"
  },
  "dependencies": {
    "@felte/solid": "^1.2.10",
    "@solid-primitives/intersection-observer": "^2.1.2",
    "@solid-primitives/media": "^2.2.3",
    "@solid-primitives/memo": "^1.3.1",
    "@solid-primitives/props": "^3.1.6",
    "@solid-primitives/storage": "^2.0.0",
    "@tanstack/solid-table": "^8.9.3",
    "@thisbeyond/solid-select": "^0.13.0",
    "classnames": "^2.3.2",
    "feather-icons": "^4.29.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "esbuild-plugin-solid": "^0.5.0",
    "glob": "^10.3.2",
    "jsdom": "^21.1.0",
    "prettier": "2.8.3",
    "solid-js": "^1.7.7",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.1.4",
    "typescript": "^5.1.6",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.2",
    "zx": "^7.2.3"
  },
  "keywords": [
    "solid"
  ],
  "browser": {},
  "exports": {
    "./ui/*": {
      "types": "./dist/ui/*.d.ts",
      "import": "./src/ui/*.tsx"
    },
    "./util/*": {
      "types": "./dist/util/*.d.ts",
      "import": "./src/util/*.tsx"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.ts",
      "import": "./src/icons/*.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "ui/*": [
        "dist/ui/*"
      ],
      "util/*": [
        "dist/util/*"
      ],
      "icons/*": [
        "dist/icons/*"
      ]
    }
  }
}
